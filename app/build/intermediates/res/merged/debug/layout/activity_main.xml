            android:contentDescription="@string/testing"
            android:src="@drawable/paobu_new" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:contentDescription="@string/testing"
            android:src="@drawable/paobu_new" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:contentDescription="@string/testing"
            android:src="@drawable/paobu_new" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:contentDescription="@string/testing"
            android:src="@drawable/paobu_new" />


    </com.kylin.demo_circlelayout.CircleLayout>

</RelativeLayout>